{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "arrow-parens": 0,
    "no-param-reassign": 0,
    "import/prefer-default-export": 0
  }
}
